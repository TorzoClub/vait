    "@babel/preset-typescript": "^7.24.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "babel-eslint": "^10.1.0",
    "depd": "^1.1.2",
    "eslint": "^8.57.0",
    "jest": "^28.1.0",
    "rollup": "^2.73.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^5.4.5"
