{
  "name": "vait",
  "version": "4.3.0",
  "main": "./dist/vait.common.js",
  "module": "./dist/vait.esm.js",
  "typings": "./types/index.d.ts",
  "scripts": {
    "test": "jest",
    "test-watch": "jest ./src/ --watchAll",
    "build:type": "rm -rf types/* && tsc -p ./tsconfig.type.json",
